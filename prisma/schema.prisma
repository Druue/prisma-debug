generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type SubscriptionKeys {
  auth   String
  p256dh String
}

model subscription {
  id             String           @id @default(auto()) @map("_id") @db.ObjectId
  endpoint       String
  expirationTime Float
  keys           SubscriptionKeys
}
