generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["postgresqlExtensions", "views", "nativeDistinct"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model SkillsAnnotation {
    name      String   @unique @db.Citext
    canonical String?  @db.Citext
    suggest   Boolean?
    rejectAs  String?
}

view SkillsView {
    name      String   @unique @db.Citext
    canonical String?  @db.Citext
    suggest   Boolean?
    rejectAs  String?
}
