generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["darwin"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model A {
  id   Int    @id
  name String @unique
  a    String
  b    String
  B    B[]    @relation("AtoB")

  @@unique([a, b], name: "compound")
  @@index([a], name: "A_a_idx")
}

model B {
  a   String
  b   String
  aId Int
  A   A      @relation("AtoB", fields: [aId], references: [id])

  @@id([a, b])
  @@index([aId], name: "B_aId_idx")
}
